@theme "launchpad-light"

* {
    bg0:  #f5f5f5aa; /* window and prompt bg color */
    bg1:  #eeeeee; /* entry bg color */
    bg2:  #eeeeee; /* inputbar bg color */
    bg3:  #eeeeee; /* entry border color */
    bg4:  #eeeeee; /* element selected bg color */
    fg0:  #555555; /* * text-color */
}

inputbar, prompt, entry, element {
  border-radius: 0px;
}
